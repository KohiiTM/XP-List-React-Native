{
  "scheme": "xplist",
  "name": "xp-list",
  "slug": "xp-list",
  "version": "1.0.0",
  "orientation": "portrait",
  "icon": "./assets/icon.png",
  "userInterfaceStyle": "light",
  "newArchEnabled": true,
  "splash": {
    "image": "./assets/splash-icon.png",
    "resizeMode": "contain",
    "backgroundColor": "#ffffff"
  },
  "extra": {
    "eas": {
      "projectId": "916d292a-6b8c-417b-9d39-9e7e0c7f51a5"
    },
    "router": {}
  },
  "ios": {
    "bundleIdentifier": "dev.kohii.xplist",
    "supportsTablet": true,
    "infoPlist": {
      "ITSAppUsesNonExemptEncryption": false
    }
  },
  "android": {
    "adaptiveIcon": {
      "foregroundImage": "./assets/xplist-icon.png",
      "backgroundColor": "#ffffff"
    },
    "edgeToEdgeEnabled": true
  },
  "web": {
    "favicon": "./assets/xplist-icon.png"
  },
  "plugins": [
    "expo-router"
  ]
}
